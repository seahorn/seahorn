{
    "version": 6,
    "configurePresets": [
        {
            "name": "default",
            "hidden": true,
            "inherits": [
                "defaultPresets"
            ],
            "environment": {
                "Z3_ROOT": "<Z3_ROOT>",
                "YICES2_HOME": "<YICES_ROOT>",
                "CMAKE_INSTALL_PREFIX": "run",
                "CMAKE_CXX_COMPILER": "clang++-14",
                "CMAKE_C_COMPILER": "clang-14",
                "SEA_ENABLE_LLD": "ON",
                "CPACK_GENERATOR": "TGZ",
                "CMAKE_EXPORT_COMPILE_COMMANDS": "ON"
            }
        },
        {
            "name": "Debug",
            "inherits": [
                "default"
            ],
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",
                "Z3_ROOT": "$env{Z3_ROOT}",
                "YICES2_HOME": "$env{YICES2_HOME}",
                "CMAKE_INSTALL_PREFIX": "$env{CMAKE_INSTALL_PREFIX}",
                "CMAKE_CXX_COMPILER": "$env{CMAKE_CXX_COMPILER}",
                "CMAKE_C_COMPILER": "$env{CMAKE_C_COMPILER}",
                "SEA_ENABLE_LLD": "$env{SEA_ENABLE_LLD}",
                "CPACK_GENERATOR": "$env{CPACK_GENERATOR}",
                "CMAKE_EXPORT_COMPILE_COMMANDS": "$env{CMAKE_EXPORT_COMPILE_COMMANDS}"
            }
        },
        {
            "name": "RelWithDebInfo",
            "inherits": [
                "default"
            ],
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "RelWithDebInfo",
                "Z3_ROOT": "$env{Z3_ROOT}",
                "YICES2_HOME": "$env{YICES2_HOME}",
                "CMAKE_INSTALL_PREFIX": "$env{CMAKE_INSTALL_PREFIX}",
                "CMAKE_CXX_COMPILER": "$env{CMAKE_CXX_COMPILER}",
                "CMAKE_C_COMPILER": "$env{CMAKE_C_COMPILER}",
                "SEA_ENABLE_LLD": "$env{SEA_ENABLE_LLD}",
                "CPACK_GENERATOR": "$env{CPACK_GENERATOR}",
                "CMAKE_EXPORT_COMPILE_COMMANDS": "$env{CMAKE_EXPORT_COMPILE_COMMANDS}"
            }
        },
        {
            "name": "Coverage",
            "inherits": [
                "default"
            ],
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Debug",
                "Z3_ROOT": "$env{Z3_ROOT}",
                "YICES2_HOME": "$env{YICES2_HOME}",
                "CMAKE_INSTALL_PREFIX": "$env{CMAKE_INSTALL_PREFIX}",
                "CMAKE_CXX_COMPILER": "$env{CMAKE_CXX_COMPILER}",
                "CMAKE_C_COMPILER": "$env{CMAKE_C_COMPILER}",
                "SEA_ENABLE_LLD": "$env{SEA_ENABLE_LLD}",
                "CPACK_GENERATOR": "$env{CPACK_GENERATOR}",
                "CMAKE_EXPORT_COMPILE_COMMANDS": "$env{CMAKE_EXPORT_COMPILE_COMMANDS}"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "Debug",
            "configurePreset": "Debug"
        },
        {
            "name": "RelWithDebInfo",
            "configurePreset": "RelWithDebInfo"
        },
        {
            "name": "Coverage",
            "configurePreset": "Coverage"
        }
    ]
}
